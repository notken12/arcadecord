{
  "type": "module",
  "name": "arcadecord",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "worker": "npm run start",
  "start": "npm run start",
  "test": "npm run start",
  "scripts": {
    "build": "npx vite build",
    "startbot": "concurrently \"nodemon --watch bot bot/proxy.js\" \"nodemon --watch bot bot/index.js 1\" \"nodemon --watch bot bot/index.js 2\"",
    "startserver": "concurrently \"nodemon --watch server server/index.js 1\" \"nodemon --watch server server/index.js 2\" \"nodemon --watch server server/index.js 3\"",
    "start": "concurrently \"npm run startserver\" \"npm run startbot\"",
    "startprod": "concurrently \"node server/index.js 1\" \"node bot/proxy.js\" \"node bot/index.js 1\" \"node bot/index.js 2\"",
    "startsingle": "concurrently \"nodemon --watch server server/index.js single\" \"nodemon --watch bot bot/proxy.js\" \"nodemon --watch bot bot/index.js 1\" \"nodemon --watch bot bot/index.js 2\"",
    "format": "prettier --write '**/*.js'"
  },
  "engines": {
    "node": "16.x"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "nodemonConfig": {
    "ignore": [
      "build/*",
      "server/src/*",
      "server/games/types/*/index.html"
    ],
    "watch": [
      "server/src/games/**"
    ],
    "delay": 2500
  },
  "dependencies": {
    "@discordjs/builders": "^0.9.0",
    "@discordjs/rest": "^0.1.0-canary.0",
    "@microsoft/applicationinsights-web": "^2.7.2",
    "@socket.io/redis-adapter": "^7.1.0",
    "ajv": "^8.10.0",
    "applicationinsights": "^2.2.0",
    "bases": "^0.2.1",
    "canvas-confetti": "^1.4.0",
    "concurrently": "^7.0.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dat.gui": "^0.7.7",
    "discord-api-types": "^0.24.0",
    "discord.js": "^13.5.0",
    "dotenv": "^10.0.0",
    "enable3d": "^0.25.0",
    "express": "^4.17.1",
    "find-up": "^5.0.0",
    "gsap": "^3.9.1",
    "jsonwebtoken": "^8.5.1",
    "lodash.clonedeep": "^4.5.0",
    "mitt": "^3.0.0",
    "mongoose": "^6.1.0",
    "redis": "^4.0.2",
    "remarkable": "^2.0.1",
    "shrink-ray-current": "^4.1.3",
    "snowflake-generator": "^1.0.2",
    "socket.io": "^4.4.1",
    "socket.io-client": "^4.4.1",
    "vue": "^3.2.27",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@vitejs/plugin-vue": "^2.0.1",
    "nodemon": "^2.0.15",
    "prettier": "^2.0.4",
    "rollup-plugin-brotli": "^3.1.0",
    "rollup-plugin-graph": "^2.0.0",
    "rollup-plugin-visualizer": "^5.5.4",
    "sass": "^1.48.0",
    "vite": "^2.7.13",
    "vite-plugin-handlebars": "^1.6.0"
  }
}
