{
  "type": "module",
  "name": "arcadecord",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "worker": "npm run start",
  "start": "npm run start",
  "test": "npm run start",
  "scripts": {
    "build": "npx snowpack build",
    "startbot": "concurrently \"nodemon --watch bot bot/proxy.js\" \"nodemon --watch bot bot/index.js 1\" \"nodemon --watch bot bot/index.js 2\"",
    "startserver": "concurrently \"nodemon --watch server server/index.js 1\" \"nodemon --watch server server/index.js 2\" \"nodemon --watch server server/index.js 3\"",
    "start": "concurrently \"npm run startserver\" \"npm run startbot\"",
    "format": "prettier --write '**/*.js'"
  },
  "engines": {
    "node": "16.x"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "nodemonConfig": {
    "ignore": [
      "build/*",
      "server/src/*",
      "server/games/*/index.html"
    ],
    "delay": 2500
  },
  "dependencies": {
    "@discordjs/builders": "^0.9.0",
    "@discordjs/rest": "^0.1.0-canary.0",
    "applicationinsights": "^2.2.0",
    "bases": "^0.2.1",
    "cannon-es": "^0.18.0",
    "canvas": "^2.8.0",
    "concurrently": "^7.0.0",
    "cookie-parser": "^1.4.5",
    "dat.gui": "^0.7.7",
    "discord-api-types": "^0.24.0",
    "discord.js": "^13.5.0",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "http-proxy": "^1.18.1",
    "lock": "^1.1.0",
    "lodash.clonedeep": "^4.5.0",
    "mongoose": "^6.1.0",
    "pts": "^0.10.6",
    "request": "^2.88.2",
    "sequelize": "^6.8.0",
    "snowflake-generator": "^1.0.2",
    "socket.io": "^4.3.1",
    "socket.io-client": "^4.4.1",
    "socket.io-redis": "^6.1.1",
    "sqlite3": "^5.0.2",
    "three": "^0.115.0"
  },
  "devDependencies": {
    "@snowpack/plugin-sass": "^1.4.0",
    "@snowpack/plugin-vue": "^2.6.2",
    "@vitejs/plugin-vue": "^2.0.1",
    "nodemon": "^2.0.15",
    "prettier": "^2.0.4",
    "snowpack": "^3.8.8",
    "vite": "^2.7.10",
    "vite-plugin-mix": "^0.3.1",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.2"
  }
}
